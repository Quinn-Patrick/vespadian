function lmTileSwitch(argument0) {
	lmId = real(argument0);

	switch(lmId){
		case 0:
		return spr_cityTile;
		break;
		case 1:
		return spr_eladarMantaTile;
		break;
		case 2:
		return spr_feyTile;
		break;
		case 3:
		return spr_mountainLocation;
		break;
		case 4:
		return spr_shackTile;
		break;
		case 5:
		return spr_ruinsLocation;
		break;
		case 6:
		return spr_vanandelTile;
		break;
		case 7:
		return spr_secretHillTile;
		break;
	}


}
